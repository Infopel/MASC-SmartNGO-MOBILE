{
  "name": "SmartNGO",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "bundle": "yarn bundle:ios && yarn bundle:android",
    "bundle:ios": "react-native rnx-bundle --platform ios ...",
    "bundle:android": "react-native rnx-bundle --platform android ...",
    "start": "react-native rnx-start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "test:e2e": "detox test -c android.emu.debug",
    "build:e2e": "detox build -c android.emu.debug",
    "ci:test:e2e": "detox test -c android.emu.release -l verbose --cleanup",
    "ci:build:e2e": "detox build -c android.emu.release",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm run format:js && npm run format:json && npm run format:md && npm run format:ts",
    "format:js": "prettier --write \"app/**/*.js\"",
    "format:json": "prettier --write \"app/**/*.json\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:ts": "prettier --write \"app/**/*.ts{,x}\"",
    "lint": "eslint index.js app storybook test --fix --ext .js,.ts,.tsx && npm run format",
    "patch": "patch-package",
    "storybook": "start-storybook -p 9001 -c ./storybook",
    "test": "jest --config ./jest.config.ts",
    "test:w": "jest --config ./jest.config.ts --watch --runInBand",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "postinstall": "node ./bin/postInstall",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle ",
    "clean": "react-native-clean-project",
    "clean-all": "npx react-native clean-project-auto"
  },
